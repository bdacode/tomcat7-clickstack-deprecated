            "MYSQL_PASSWORD_MYDB2": "pass",
            "MYSQL_USERNAME_MYDB2": "test2-db",
            "MYSQL_URL_MYDB2": "mysql://localhost:3306/my-test2-db"
        }
    },
    "tomcat7": {
        
    },
