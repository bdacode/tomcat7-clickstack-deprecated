            {
                "name": "tomcat7",
                "src": "http:\/\/cloudbees-clickstack.s3.amazonaws.com\/core\/tomcat7-plugin.zip"
            }
        ],
        "env": {
            "MYSQL_PASSWORD_MYDB": "pass",
            "MYSQL_MAXACTIVE_MYDB": "10",
